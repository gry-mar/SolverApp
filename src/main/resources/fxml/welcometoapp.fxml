<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="anchor" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ib.WelcomeController">
   <stylesheets>
      <URL value="@style_welcome.css" />
   </stylesheets>
   <children>
      <Text fill="#130f2f" layoutX="185.0" layoutY="121.0" lineSpacing="1.2" stroke="#282a51e0" strokeLineJoin="ROUND" strokeType="OUTSIDE" strokeWidth="1.2" text="Welcome to Solver">
         <font>
            <Font name="Commercial Pi BT" size="28.0" />
         </font>
      </Text>
      <Button fx:id="btnGoToSolver" layoutX="239.0" layoutY="176.0" mnemonicParsing="false" onAction="#goToSolverClicked" prefHeight="49.0" prefWidth="121.0" style="-fx-background-color: #480eab;" text="Go to Solver" textFill="WHITE" />
      <Button fx:id="btnCheckNotes" layoutX="223.0" layoutY="267.0" mnemonicParsing="false" onAction="#checkNotesClicked" prefHeight="41.0" prefWidth="154.0" style="-fx-background-color: #480eab;" text="Check notes" textFill="WHITE" />
   </children>
